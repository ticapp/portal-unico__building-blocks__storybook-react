{
  "name": "@ama/react-design-system",
  "description": "AMA Design system",
  "version": "0.2.13",
  "publishConfig": {
    "access": "public",
    "registry": "https://ticappsonar.northeurope.cloudapp.azure.com/nexus/repository/npm-general"
  },
  "main": "dist/cjs/components/index.js",
  "module": "dist/esm/components/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch-sass": "sass --load-path=./ --watch ./src/scss/index.scss ./src/scss/index.min.css",
    "start-storybook": "start-storybook -p 6006",
    "start": "npm-run-all --parallel watch-sass start-storybook",
    "build-sass": "sass --load-path=./ ./src/scss/index.scss ./src/scss/index.min.css",
    "generate-storybook": "build-storybook",
    "build-storybook": "npm run build-sass && npm run generate-storybook",
    "clean": "gulp clean",
    "build:styles": "gulp build",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build": "npm run clean && npm run build:styles && npm run build:esm && npm run build:cjs",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix ./src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,css,scss,mdx,md,json} --config ./.prettierrc"
  },
  "author": "TicApp",
  "license": "MIT",
